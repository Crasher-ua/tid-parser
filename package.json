{
  "name": "tid-parser",
  "version": "0.2.0",
  "description": "TID parser",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "del": "^1.2.1",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "jquery": "^3.3.1",
    "jsdom": "^9.12.0",
    "nodemon": "^1.17.2",
    "run-sequence": "^2.2.1",
