    "webpack": "^3.11.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "scripts": {
    "client-build": "cd mean/client/modules/engine && gulp",
